
@model ors_head_request

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h2>Update entry</h2>
            <form action="@Url.Action("UpdateOrsHeadRequest","ORS")" method="post">
                @Html.AntiForgeryToken()
                <table class="table">
                    <tr>
                        <td>Name</td>
                        <td><input type="text" name="name" class="form-control" required value="@Model.Name" /></td>
                    </tr>
                    <tr>
                        <td>Position</td>
                        <td><input type="text" name="position" class="form-control" required value="@Model.Position" /></td>
                    </tr>
                    <tr>
                        <td></td>
                        <td>
                            <button type="submit" class="btn btn-success btn-lg" name="submit" value="Save Changes">Submit</button>
                            <button type="button" class="btn btn-default btn-lg" data-dismiss="modal">Close</button>
                        </td>
                    </tr>
                </table>
            </form>
        </div>
    </div>
</div>
@section footer{
    @Html.Partial("footer")
}

