

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h3>ORS Reports</h3>
            <form role="form" action="@Url.Action("ORS_PAGE","ORSReports")" method="post">
                @Html.AntiForgeryToken()
                <table>
                    <tr>
                        <td>Select ORS</td>
                        <td>
                            <select name="ors" class="form-control" onchange="getFS(this);" data-url="@Url.Action("getFS","ORSReports")">
                                <option value="" selected disabled>Select Value</option>
                                @foreach (Allotments allotment in Model)
                                {
                                    <option value="@allotment.ID">@allotment.Code</option>
                                }
                            </select>
                        </td>
                    </tr>
                    <tr>
                        <td>Page</td>
                        <td><input type="text" name="page" required class="form-control" placeholder="Exp. 1-20" /></td>
                    </tr>
                    <tr>
                        <td></td>
                        <td><button type="submit" class="btn btn-success btn-lg"> <span class="glyphicon glyphicon-print"></span> Print</button></td>
                    </tr>
                </table>
            </form>
        </div>
    </div>
</div>
